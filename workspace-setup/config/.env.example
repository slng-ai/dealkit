# Baseten Sales Workspace Environment Variables
# Copy this file to .env and fill in your actual credentials

# Vault Configuration (for secure credential management)
VAULT_URL=https://vault.company.com
VAULT_TOKEN=your-vault-token-here

# Slack Integration
SLACK_WORKSPACE=baseten-team
SLACK_BOT_TOKEN=xoxb-your-bot-token-here
SLACK_USER_TOKEN=xoxp-your-user-token-here
SLACK_SIGNING_SECRET=your-signing-secret-here

# Gong Integration
GONG_ACCESS_KEY=your-gong-access-key
GONG_ACCESS_SECRET=your-gong-access-secret
GONG_WEBHOOK_SECRET=your-webhook-secret

# Granola Integration
GRANOLA_API_ENDPOINT=https://api.granola.com/v1
GRANOLA_API_KEY=your-granola-api-key

# Gmail Integration
GMAIL_CLIENT_ID=your-gmail-client-id
GMAIL_CLIENT_SECRET=your-gmail-client-secret

# Salesforce Integration
SFDC_INSTANCE_URL=https://baseten.my.salesforce.com
SFDC_CLIENT_ID=your-salesforce-client-id
SFDC_CLIENT_SECRET=your-salesforce-client-secret
SFDC_USERNAME=your-salesforce-username
SFDC_SECURITY_TOKEN=your-salesforce-security-token

# Supabase Integration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_KEY=your-supabase-service-role-key
SUPABASE_ANON_KEY=your-supabase-anon-key

# Notion Integration
NOTION_API_KEY=secret_your-notion-integration-token
NOTION_WORKSPACE_ID=your-workspace-id
NOTION_CUSTOMERS_DB=customer-database-id
NOTION_DEALS_DB=deals-database-id
NOTION_REPORTS_DB=reports-database-id

# Grafana Integration
GRAFANA_URL=https://grafana.company.com
GRAFANA_API_KEY=your-grafana-api-key
GRAFANA_ORG_ID=1

# GitHub Integration
GITHUB_TOKEN=ghp_your-github-personal-access-token

# LinkedIn Sales Navigator
LINKEDIN_SESSION_COOKIE=your-linkedin-session-cookie
LINKEDIN_CSRF_TOKEN=your-linkedin-csrf-token

# Google Workspace
GOOGLE_CLIENT_ID=your-google-oauth-client-id
GOOGLE_CLIENT_SECRET=your-google-oauth-client-secret

# Database Configuration
REDIS_URL=redis://localhost:6379
ELASTICSEARCH_URL=https://elasticsearch.company.com:9200

# Monitoring & Alerting
PAGERDUTY_API_KEY=your-pagerduty-integration-key
SENDGRID_API_KEY=SG.your-sendgrid-api-key
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
FIREBASE_SERVER_KEY=your-firebase-server-key

# Security
ENCRYPTION_KEY=your-32-character-encryption-key
JWT_SECRET=your-jwt-secret-for-authentication

# Environment
NODE_ENV=production
LOG_LEVEL=info